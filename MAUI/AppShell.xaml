<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUI"
    Shell.FlyoutBehavior="Disabled"
    Title="MAUI">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:JoinPage}"
        Route="joinpage" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:GamePage}"
        Route="gamepage" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:VictoriaPage}"
        Route="victoriapage" />

</Shell>
